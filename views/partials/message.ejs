<% if(typeof errors != 'undefined') {
    errors.forEach(function(error){
%>
<div class="alert alert-warning p-2">
    <%= error.msg %>
</div>
<%
    })
 }
%>

<% if(success_msg != ''){ %>
    <div class="alert alert-success p-2">
        <%= success_msg %>
    </div>
<% } %>

<% if(error != ''){ %>
    <div class="alert alert-success p-2">
        <%= error %>
    </div>
<% } %>